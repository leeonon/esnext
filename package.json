{
  "name": "esnext",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "db:push": "pnpm --filter @esnext/site db:push",
    "db:migrate": "pnpm --filter @esnext/site db:migrate",
    "db:migrate:apply": "pnpm --filter @esnext/site db:migrate:apply",
    "db:generate": "pnpm --filter @esnext/site db:generate",
    "db:format": "pnpm --filter @esnext/site db:format",
    "dev:site": "pnpm --filter @esnext/site dev",
    "start:site": "pnpm --filter @esnext/site start",
    "prepare": "husky install",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint": "pnpm lint:lint-staged",
    "log": "npx conventional-changelog --config ./node_modules/@commitlint/cli -i CHANGELOG.md -s -r 0"
  },
  "keywords": [],
  "author": "yang",
  "license": "ISC",
  "peerDependencies": {
    "@types/node": "*"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@types/eslint": "^8.44.2",
    "@types/node": "^18.16.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.47.0",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.1",
    "prisma": "^5.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0-beta"
  },
  "dependencies": {
    "@nextui-org/react": "^2.1.13",
    "@prisma/client": "5.3.1",
    "dayjs": "^1.11.10",
    "framer-motion": "^10.16.4"
  }
}
