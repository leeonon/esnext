{
  "name": "esnext",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "db:push": "pnpm --filter @esnext/site db:push",
    "db:migrate": "pnpm --filter @esnext/site db:migrate",
    "db:migrate:apply": "pnpm --filter @esnext/site db:migrate:apply",
    "db:generate": "pnpm --filter @esnext/site db:generate",
    "db:format": "pnpm --filter @esnext/site db:format",
    "dev:site": "pnpm --filter @esnext/site dev",
    "start:site": "pnpm --filter @esnext/site start",
    "prepare": "husky install",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:style": "stylelint **/*.css",
    "lint": "pnpm lint:lint-staged",
    "commit": "git-cz",
    "test": "vitest --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "yang",
  "license": "ISC",
  "peerDependencies": {
    "@types/node": "*"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@commitlint/types": "^18.0.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/eslint": "^8.44.2",
    "@types/node": "^18.16.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/ui": "^0.34.6",
    "commitizen": "^4.3.0",
    "cz-git": "^1.7.1",
    "eslint": "^8.47.0",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.1",
    "prisma": "^5.1.1",
    "stylelint": "^15.11.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0-beta",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@nextui-org/react": "0.0.0-dev-v2-20231016135001",
    "@prisma/client": "5.3.1",
    "dayjs": "^1.11.10",
    "framer-motion": "^10.16.4",
    "fuse.js": "^6.6.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
