{
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
  },
  "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact",
  ],
  "eslint.alwaysShowStatus": true,
  "stylelint.validate": [
      "css",
      "less",
      "postcss",
      "scss",
      "sass"
  ],
  "cSpell.words": [
    "iconify",
    "nextjs",
    "nextui",
    "preinstall",
    "stylelint",
    "trpc"
  ],
  "typescript.tsdk": "node_modules/typescript/lib"
}
